             let auxCategorias = await CategoriasCollection.doc(product.data().categoria).get()\r\n                // console.log(auxCategorias.data())\r\n                return { ...product.data(), categoria:auxCategorias.data().category, id: product.id }\r\n            })); \r\n            setDetallePro(...aux.filter(element=> element.id.toString() === id));    \r\n        })\r\n    }, []);\r\n\r\n    console.log(detallePro)\r\n\r\n    return (\r\n        <section>\r\n            {detallePro === undefined ? <NotFound/> :\r\n            <ItemDetail detallePro={detallePro}/>  }\r\n            <Link to={'/'} className={classes.link}>vuelve a ver todos nuestros productos</Link>\r\n        </section>\r\n    )\r\n}\r\n\r\nexport default ItemDetailContainer;\r\n","C:\\Users\\David\\Desktop\\proyecto react js\\proyecto-david-angel\\src\\components\\ItemDetail\\index.jsx",[],"C:\\Users\\David\\Desktop\\proyecto react js\\proyecto-david-angel\\src\\Context\\cartContext\\index.jsx",[],"C:\\Users\\David\\Desktop\\proyecto react js\\proyecto-david-angel\\src\\components\\cart\\cart.jsx",[],"C:\\Users\\David\\Desktop\\proyecto react js\\proyecto-david-angel\\src\\components\\cartItem\\index.jsx",[],"C:\\Users\\David\\Desktop\\proyecto react js\\proyecto-david-angel\\src\\firebase\\index.jsx",[],"C:\\Users\\David\\Desktop\\proyecto react js\\proyecto-david-angel\\src\\components\\emptyCart\\index.jsx",[],"C:\\Users\\David\\Desktop\\proyecto react js\\proyecto-david-angel\\src\\components\\checkout\\index.jsx",["119"],"import React, { useContext } from 'react';\r\n//mui\r\nimport TextField from '@material-ui/core/TextField';\r\nimport { makeStyles } from '@material-ui/core/styles';\r\nimport Snackbar from '@material-ui/core/Snackbar';\r\nimport Alert from '@material-ui/lab/Alert';\r\n\r\n//propios\r\nimport styles from './check.module.scss'\r\nimport { cartContext } from '../../Context/cartContext';\r\nimport CheckCart from '../checkcart';\r\nimport Typography from '@material-ui/